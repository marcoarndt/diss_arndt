@article{Arndt.2023c,
 abstract = {For the investigation of influence of various parameters on properties and outputs of components or systems, Design of Experiments (DOE) offers the most efficient approach to create a{\&}nbsp;comprehensive empirical insight into product performance. However, especially if product lifetime is treated as the investigation objective, the main focus of attention must be placed on the efficiency of testing---if only to comply with the principle of DOE, even before testing begins. Without actual test runs, a{\&}nbsp;pre-selection of relevant factors influencing the target quantity can be performed here and strategically adjusted in scale compared to the subsequent method. In this work, common heuristic tools and methods are analyzed and evaluated with respect to a{\&}nbsp;deliberate preselection of influencing factors versus the challenges in lifetime testing and degradation behaviors. Several factors as well as their interactions are taken into account to achieve this. For this purpose, these methods are partially extended and adapted in their focus in order to finally be made applicable in a{\&}nbsp;suitable procedure. An illustration of this is also provided in a{\&}nbsp;selected use case with limited empirical and experimental prior-knowledge, in which a{\&}nbsp;sample of relevant influences is identified through qualitative heuristic decision making with respect to parameters that influence product lifetime.},
 author = {Arndt, Marco and Dazer, Martin and Raither, Wolfram and Bertsche, Bernd},
 year = {2023},
 title = {Parameter assessment for reliability modeling of machine components using heuristic screening},
 url = {https://link.springer.com/article/10.1007/s10010-023-00711-5#citeas},
 pages = {1347--1370},
 volume = {87},
 number = {4},
 issn = {0015-7899},
 journal = {Forschung im Ingenieurwesen},
 doi = {10.1007/s10010-023-00711-5},
 file = {Arndt, Dazer et al. 2023 - Parameter assessment for reliability modeling (5):Attachments/Arndt, Dazer et al. 2023 - Parameter assessment for reliability modeling (5).pdf:application/pdf}
}


